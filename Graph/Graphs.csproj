<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

    <PackageId>Graphs</PackageId>
    <Version>1.0.0</Version>
    <Authors>Nick Harvey</Authors>
    <Company></Company>
    <description>A generic collection data structure for tracking relationships between objects</description>
    <PackageLicense>https://mit-license.org/</PackageLicense>
    <PackageLicenseUrl>https://mit-license.org/</PackageLicenseUrl>

    <DebugType>Full</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
  </ItemGroup>

</Project>
